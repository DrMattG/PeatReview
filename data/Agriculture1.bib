@article{ ISI:000404779600016,
Author = {Demasi, Sonia and Caser, Matteo and Handa, Takashi and Kobayashi, Nobuo
   and De Pascale, Stefania and Scariot, Valentina},
Title = {{Adaptation to iron deficiency and high pH in evergreen azaleas
   (Rhododendron spp.): potential resources for breeding}},
Journal = {{EUPHYTICA}},
Year = {{2017}},
Volume = {{213}},
Number = {{7}},
Month = {{JUL}},
Note = {{20th EUCARPIA General Congress, Zurich, SWITZERLAND, AUG 29-SEP 01, 2016}},
Organization = {{EUCARPIA}},
Abstract = {{The growth of evergreen azaleas (Rhododendron spp.) can be altered by
   iron (Fe) chlorosis when plants are cultivated in a neutral-alkaline
   substrate. In this study, morphological and physiological responses to
   alkalinity and Fe deficiency were evaluated in five diploid Japanese
   azaleas to assess their potential as resources for breeding. R. obtusum
   `Kirin', R. indicum `Shinsen', R. x pulchrum `Sen-e- oomurasaki', R.
   indicum `Osakazuki', and R. ripense were pot cultivated in a peat-based
   substrate for 10 weeks, in acid and alkaline growing media with both
   adequate and inadequate Fe nutrition. Plant performance was generally
   affected by high pH of the substrate, while Fe deficiency by itself
   influenced few of the evaluated parameters, possibly due to the complex
   adaptive response mechanisms of these slow growing ornamental shrubs.
   According to the biochemical and physiological variations recorded on a
   long period of cultivation, R. indicum `Osakazuki' reported the best
   performance. This azalea could be a valuable resource for breeders.}},
