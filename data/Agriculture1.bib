@article{ ISI:000348381700009,
Author = {Sirin, A. A. and Maslov, A. A. and Valyaeva, N. A. and Tsyganova, O. P.
   and Glukhova, T. V.},
Title = {{Mapping of peatlands in the Moscow oblast based on high-resolution
   remote sensing data}},
Journal = {{CONTEMPORARY PROBLEMS OF ECOLOGY}},
Year = {{2014}},
Volume = {{7}},
Number = {{7}},
Pages = {{808-814}},
Month = {{DEC}},
Abstract = {{Peatlands, including those transformed by economic activities (drainage
   for forestry and agriculture, peat extraction), belong to various land
   categories. There is no general system for their inventory and
   accounting. Remote sensing methodology based on high-resolution space
   imagery is proposed and applied for mapping peatlands of different types
   and states based on the example of Moscow oblast. The methodology can be
   used to solve scientific and practical tasks that require the
   development of a regional peatland geoin-formation system (GIS) using a
   common base map. The total area of peatlands and peatbogs is shown to
   surpass 6\% of the oblast territory.}},
