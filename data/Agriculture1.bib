@article{ ISI:000229287500014,
Author = {Koskiaho, J and Puustinen, M},
Title = {{Function and potential of constructed wetlands for the control of N and
   P transport from agriculture and peat production in boreal climate}},
Journal = {{JOURNAL OF ENVIRONMENTAL SCIENCE AND HEALTH PART A-TOXIC/HAZARDOUS
   SUBSTANCES \& ENVIRONMENTAL ENGINEERING}},
Year = {{2005}},
