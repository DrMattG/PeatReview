@article{ ISI:000339935500007,
Author = {Truskavetskii, R. S.},
Title = {{Carbon budget of drained peat bogs in Ukrainian Polesie}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{2014}},
Volume = {{47}},
Number = {{7}},
Pages = {{687-693}},
Month = {{JUL}},
Abstract = {{Methodological aspects of studying the organic carbon budget of drained
   peat bogs used in agriculture are discussed. Difficulties in the
   assessment of carbon budget on the basis of measured carbon inflows and
   outflows are shown. The ``soil pool{''} model of the carbon budget is
   suggested. It is based on the results of long-term experimental studies
   of a drained peatland in Ukrainian Polesie. An algorithm for calculating
   the carbon budget in the aerated part of the peatland-the peat soil
   proper-is developed with due account for a gradual involvement of the
   deep peat layers into the zone of soil processes. Data on the loss of
