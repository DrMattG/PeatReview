@article{ ISI:000377360200003,
Author = {Jones, Timothy G. and Evans, Chris D. and Jones, Davey L. and Hill, Paul
   W. and Freeman, Chris},
Title = {{Transformations in DOC along a source to sea continuum; impacts of
   photo-degradation, biological processes and mixing}},
Journal = {{AQUATIC SCIENCES}},
