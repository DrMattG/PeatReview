@article{ ISI:000407955800041,
Author = {Luchanok, Ludmila and Ptashets, Olga},
Title = {{THE CULTIVATION OF LUCERNE ON MINERALIZED PEAT SOILS IN BELARUS}},
Journal = {{SCIENTIFIC PAPERS-SERIES MANAGEMENT ECONOMIC ENGINEERING IN AGRICULTURE
   AND RURAL DEVELOPMENT}},
Year = {{2016}},
Volume = {{16}},
Number = {{1}},
Pages = {{265-270}},
Abstract = {{The data on efficiency of a lucerne (Medicago sativa) of the sowing
   campaign cultivated on the antropogenically transformed peat soils of
   Polesye with the content of organic substance of 13-26\% and the level
   of ground waters more than 0.8 m are submitted. Productivity of lucerne
   herbage can reach depending on applied agro- and biotechnological
   methods. It is established that efficiency of a lucerne herbage can
   reach 56.8-87.9 centner feed units per hectare depending on the applied
   agrobioprocessing methods. At cultivation of a lucerne on such soils
   without cover culture (the doubled norm of seeding) more effective in
   comparison than under winter pea and oat mix. This mode provides an
   increasing of feed units per hectare by 13.8-26.1; digested protein
   content by 1.4-2.6 and exchangeable energy by 15.83-30.61 GJ per
   hectare.}},
