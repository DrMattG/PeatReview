@article{ ISI:000338206900004,
Author = {Bork, S. P. and Pypker, T. G. and Corace, III, R. G. and Chimner, R. A.
   and Maclean, A. L. and Hribljan, J. A.},
Title = {{A Case Study in Large-scale Wetland Restoration at Seney National
   Wildlife Refuge, Upper Michigan, USA}},
Journal = {{AMERICAN MIDLAND NATURALIST}},
