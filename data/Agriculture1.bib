@article{ ISI:000228334800002,
Author = {Bullard, GK and Roughley, RJ and Pulsford, DJ},
Title = {{The legume inoculant industry and inoculant quality control in
   Australia: 1953-2003}},
Journal = {{AUSTRALIAN JOURNAL OF EXPERIMENTAL AGRICULTURE}},
