@article{ ISI:000428097500016,
Author = {O'Driscoll, Connie and Sheahan, Jerome and Renou-Wilson, Florence and
   Croot, Peter and Pilla, Francesco and Misstear, Bruce and Xiao, Liwen},
Title = {{National scale assessment of total trihalomethanes in Irish drinking
   water}},
