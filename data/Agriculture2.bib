@article{ ISI:000079828000012,
Author = {Baranovskii, AZ},
Title = {{The formation of profile in drained peaty soils under agricultural use}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{1999}},
Volume = {{32}},
Number = {{4}},
Pages = {{474-481}},
Month = {{APR}},
Abstract = {{Drainage and agricultural use of peaty soils result in the compaction of
   peat, an increase in its ash content, decrease in its water-retaining
   capacity, and peat exhaustion. The duration of these processes depends
   on depletion the original thickness of the peat deposit and the
   management practices. A decrease in thickness of the peat deposit
   approximating the thickness of the arable layer, as well as the growth
   of tilled crops, can accelerate these processes; the use of lands for
   meadows inhibits them. As the underlying horizon becomes depleted in
   organic matter, the profile of peaty soils incorporates mineral layers.
   On these soils, the yield of cereals increases, and the yield of
   perennial grasses decreases. In order to increase the grass yield, the
   depth of the groundwater table should be regulated, and additional
   nitrogen fertilizers should be applied.}},
