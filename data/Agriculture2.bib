Affiliation = {{RICHARDSON, CJ (Corresponding Author), DUKE UNIV,CTR WETLAND,SCH ENVIRONM,DURHAM,NC 27706, USA.}},
DOI = {{10.2136/sssaj1995.03615995005900060040x}},
