@article{ ISI:000167133500012,
Author = {Stein, VB and Hettiaratchi, JPA},
Title = {{Methane oxidation in three Alberta soils: Influence of soil parameters
   and methane flux rates}},
Journal = {{ENVIRONMENTAL TECHNOLOGY}},
Year = {{2001}},
Volume = {{22}},
Number = {{1}},
Pages = {{101-111}},
Month = {{JAN}},
Abstract = {{Current concern over the potentially negative impacts of climate change
   has brought attention to anthropogenic sources of methane, a primary
   greenhouse gas. Two such emission sources are methane leakage at heavy
   oil wells and sanitary landfills. At both of these sources, substantial
   quantities of methane could potentially be oxidised by methanotrophic
   microbes living in soils. Optimisation of this phenomenon may serve as
   an inexpensive technique for reducing methane emissions. Soil column and
   batch incubation experiments were performed on a landfill loam, an
   agricultural loam and a sedge peat to gain a better quantitative
   understanding of the biological and physical processes limiting CH4
   oxidation in soils that undergo the freeze-thaw cycles associated with
   northern climates. Moisture content emerged as a critical variable that
   can limit a soil's CH4 oxidation potential. For example, the oxidation
   rate of the agricultural soil was seen to increase by an order of
   magnitude after increasing its moisture content from 6\% to 10\% of its
   dry weight.}},
