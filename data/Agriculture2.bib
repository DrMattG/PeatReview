@article{ ISI:A1997XF43500019,
Author = {Sinkevich, EI},
Title = {{Reserves of soil nitrogen in drained peat soils of the European north
   and their need in nitric fertilizers}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{1997}},
Volume = {{30}},
Number = {{2}},
Pages = {{204-209}},
Month = {{FEB}},
Abstract = {{Annual mobilization of soil nitrogen in peal soils varies from 60-120 to
   300-350 kg/ha. The need in nitric fertilizers varies from very high to
   low when compared with the need in and potassium fertilizers. In some
   cases, the lowest rates of nitrogen are required, During the initial
   stages of cultivation and agricultural development of peat soils, the
   reserves of soil nitrogen may play the main role in nutrition of plants.
   After 35-50 years of cultivation, natural reserves of nitrogen in peat
   soils decrease significantly, acid the role of nitric fertilizers
   increases. At present, there is a great need in nitric fertilizers, and
   it will increase in the future.}},
