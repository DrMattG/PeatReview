@article{ ISI:000169227600003,
Author = {Patzelt, A and Wild, U and Pfadenhauer, J},
Title = {{Restoration of wet fen meadows by topsoil removal: Vegetation
   development and germination biology of fen species}},
Journal = {{RESTORATION ECOLOGY}},
