@article{ ISI:000075594000012,
Author = {Willison, TW and Baker, JC and Murphy, DV},
Title = {{Methane fluxes and nitrogen dynamics from a drained fenland neat}},
Journal = {{BIOLOGY AND FERTILITY OF SOILS}},
