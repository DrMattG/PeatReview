@article{ ISI:000082715500012,
Author = {Bergamaschi, BA and Fram, MS and Kendall, C and Silva, SR and Aiken, GR
   and Fujii, R},
Title = {{Carbon isotopic constraints on the contribution of plant material to the
   natural precursors of trihalomethanes}},
